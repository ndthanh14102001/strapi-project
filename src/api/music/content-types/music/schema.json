{
  "kind": "collectionType",
  "collectionName": "musics",
  "info": {
    "singularName": "music",
    "pluralName": "musics",
    "displayName": "Music",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "albums": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::album.album",
      "inversedBy": "music"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "audios"
      ]
    },
    "users_like": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "musics_like"
    },
    "description": {
      "type": "richtext"
    },
    "lyrics": {
      "type": "richtext"
    },
    "artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "musics_composeds"
    }
  }
}
